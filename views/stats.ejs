  <style>
    :root {
      --primary: #00ADB5;
      --accent: #393E46;
      --muted: #EEEEEE;
    }
    body {
      font-family: 'Montserrat', 'Ubuntu', sans-serif;
      background: var(--muted);
      color: var(--accent);
      margin: 0; padding:0;
    }
    .navbar {
      background: linear-gradient(90deg, #393E46 70%, #00ADB5 100%)!important;
      box-shadow: 0 2px 16px rgba(57,62,70,0.08);
      padding: 1rem 2rem;
    }
    .navbar-brand {
      font-weight: bold; letter-spacing:2px;
      font-size: 1.7rem;
      color: #fff !important;
    }
    .navbar-nav .nav-link {
      color: #EEE !important;
      margin-right: 1.2rem;
      transition: color 0.2s;
      font-weight: 500;
    }
    .navbar-nav .nav-link.active, .navbar-nav .nav-link:hover {
      color: var(--primary) !important;
    }
    .stats-section {
      padding: 60px 0 40px 0;
      background: #fff;
      min-height: 100vh;
    }
    .stats-title {
      text-align: center;
      margin-bottom: 38px;
      font-weight: 700;
      font-size: 2.3rem;
      color: #00ADB5;
      letter-spacing: 1px;
      text-shadow: 0 2px 8px #00adb543;
    }
    .stats-table-box {
      background: #f7fafb;
      border-radius: 18px;
      box-shadow: 0 3px 18px -8px #aad7da4a;
      padding: 32px 18px 18px 18px;
      margin-bottom: 38px;
      max-width: 1100px;
      margin-left: auto;
      margin-right: auto;
    }
    .stats-table-box h2 {
      color: var(--primary);
      font-size: 1.35rem;
      font-weight: 700;
      margin-bottom: 1.2rem;
      letter-spacing: 1px;
      text-align: center;
    }
    .table {
      background: #fff;
      border-radius: 12px;
      overflow: hidden;
      margin-bottom: 0;
    }
    .table thead th {
      background: #00adb5;
      color: #fff;
      font-weight: 700;
      border: none;
      font-size: 1.07rem;
      letter-spacing: 1px;
    }
    .table-striped tbody tr:nth-of-type(odd) {
      background-color: #eafcfc;
    }
    .table-striped tbody tr:nth-of-type(even) {
      background-color: #f7fafb;
    }
    .table td, .table th {
      color: #222;
      font-weight: 500;
      vertical-align: middle;
      border-top: none;
      border-bottom: 1.5px solid #e0e0e0;
      font-size: 1.01rem;
    }
    .table th[rowspan], .table td[rowspan] {
      background: #e0f7fa;
      color: #00adb5;
      font-weight: 700;
      font-size: 1.05rem;
      border-right: 2px solid #fff;
    }
    @media (max-width: 991px) {
      .stats-title { font-size: 1.5rem; }
      .stats-table-box { padding: 18px 2px; }
    }
    @media (max-width: 767px) {
      .stats-table-box { padding: 8px 0; }
      .table th, .table td { font-size: 0.93rem; }
    }
  </style>
</head>
